{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "workflowy-inbox",
  "title": "workflowy-inbox",
  "description": "Quickly add items to your Workflowy inbox. ",
  "icon": "command-icon.png",
  "author": "cameron_pak",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Add to Workflowy Inbox",
      "description": "Add a note, task, or link to your Workflowy inbox with ease.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "cross-fetch": "^4.0.0",
    "nanoid": "^5.0.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
